{
    "name": "siu-toba/framework",
    "description": "Framework de desarrollo web",
    "authors": [
        {
            "name": "SIU",
            "email": "toba@siu.edu.ar"
        }
    ],
    "repositories": [{
        "type": "vcs",
        "url": "https://repositorio.siu.edu.ar/svn/toba",
        "trunk-path": "trunk",
        "branches-path": "trunk_versiones",
        "tags-path": "versiones"
    },{
        "type": "vcs",
        "url": "https://github.com/SIU-Toba/rest"
    },{
        "type": "vcs",
        "url": "https://github.com/SIU-Toba/SecurityMultiTool"
    }],
    "config" : {
        "vendor-dir" : "./php/vendor"
    },
    "minimum-stability": "dev",
    "prefer-stable" : true,
    "require": {
        "padraic/security-multitool": "@dev",
        "ircmaxell/random-lib" : "@dev",
        "siu-toba/rest": "@dev",
        "guzzlehttp/guzzle": "5.1.0",
        "onelogin/php-saml": "2.3.0"
    }
}