SELECT 	i.proyecto as	proyecto,
						i.carpeta as 	carpeta, 
						i.item as 		item,
						i.nombre as 	nombre
				FROM 	apex_item i, apex_usuario_grupo_acc_item u
				WHERE 	(i.item = u.item)
				AND		(i.proyecto = u.proyecto )
				AND		(i.padre = '')
				AND 	(i.item <> '')
				AND 	(i.menu = 1)
				AND		(u.usuario_grupo_acc = 'admin' )
				AND		(i.proyecto = 'toba');