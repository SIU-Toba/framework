--SELECT elemento_formulario, elemento_formulario, descripcion, parametros 
--FROM apex_elemento_formulario 
--WHERE (( proyecto = 'prueba') OR (( proyecto = 'toba' ) AND (( exclusivo_toba <> 1 ) OR exclusivo_toba IS NULL) )) 
--ORDER BY 2

SELECT	o.proyecto,
			o.objeto,
			cd.identificador,
			od.proyecto,
			od.identificador,
			od.objeto_proveedor
FROM 		apex_objeto o
			INNER JOIN apex_clase_dependencias cd
				ON o.clase = cd.clase_consumidora
				AND o.clase_proyecto = cd.clase_consumidora_proyecto
			LEFT OUTER JOIN apex_objeto_dependencias od
				ON o.objeto = od.objeto_consumidor
				AND o.proyecto = od.proyecto
				AND od.identificador = cd.identificador
;